RewriteEngine On
RewriteCond %{HTTPS} !on
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}


Options -Indexes

ErrorDocument 404 https://review.adlcrm.com/app/error/404.php 
ErrorDocument 403 https://review.adlcrm.com/app/error/403.php 
ErrorDocument 500 https://review.adlcrm.com/app/error/500.php

